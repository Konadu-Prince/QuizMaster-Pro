: (state) => {\n      state.sidebarOpen = !state.sidebarOpen;\n    },\n    setSidebarOpen: (state, action) => {\n      state.sidebarOpen = action.payload;\n    },\n    setLoading: (state, action) => {\n      state.loading = action.payload;\n    },\n    addNotification: (state, action) => {\n      state.notifications.push({\n        id: Date.now(),\n        ...action.payload,\n      });\n    },\n    removeNotification: (state, action) => {\n      state.notifications = state.notifications.filter(\n        notification => notification.id !== action.payload\n      );\n    },\n    clearNotifications: (state) => {\n      state.notifications = [];\n    },\n  },\n});\n\nexport const {\n  toggleTheme,\n  setTheme,\n  toggleSidebar,\n  setSidebarOpen,\n  setLoading,\n  addNotification,\n  removeNotification,\n  clearNotifications,\n} = uiSlice.actions;\n\nexport default uiSlice.reducer;\n"],"mappings":"wGAAA,OAASA,WAAW,KAAQ,kBAAkB,CAE9C,KAAM,CAAAC,YAAY,CAAG,CACnBC,KAAK,CAAE,OAAO,CACdC,WAAW,CAAE,KAAK,CAClBC,OAAO,CAAE,KAAK,CACdC,aAAa,CAAE,EACjB,CAAC,CAED,KAAM,CAAAC,OAAO,CAAGN,WAAW,CAAC,CAC1BO,IAAI,CAAE,IAAI,CACVN,YAAY,CACZO,QAAQ,CAAE,CACRC,WAAW,CAAGC,KAAK,EAAK,CACtBA,KAAK,CAACR,KAAK,CAAGQ,KAAK,CAACR,KAAK,GAAK,OAAO,CAAG,MAAM,CAAG,OAAO,CAC1D,CAAC,CACDS,QAAQ,CAAEA,CAACD,KAAK,CAAEE,MAAM,GAAK,CAC3BF,KAAK,CAACR,KAAK,CAAGU,MAAM,CAACC,OAAO,CAC9B,CAAC,CACDC,aAAa,CAAGJ,KAAK,EAAK,CACxBA,KAAK,CAACP,WAAW,CAAG,CAACO,KAAK,CAACP,WAAW,CACxC,CAAC,CACDY,cAAc,CAAEA,CAACL,KAAK,CAAEE,MAAM,GAAK,CACjCF,KAAK,CAACP,WAAW,CAAGS,MAAM,CAACC,OAAO,CACpC,CAAC,CACDG,UAAU,CAAEA,CAACN,KAAK,CAAEE,MAAM,GAAK,CAC7BF,KAAK,CAACN,OAAO,CAAGQ,MAAM,CAACC,OAAO,CAChC,CAAC,CACDI,eAAe,CAAEA,CAACP,KAAK,CAAEE,MAAM,GAAK,CAClCF,KAAK,CAACL,aAAa,CAACa,IAAI,CAAAC,aAAA,EACtBC,EAAE,CAAEC,IAAI,CAACC,GAAG,CAAC,CAAC,EACXV,MAAM,CAACC,OAAO,CAClB,CAAC,CACJ,CAAC,CACDU,kBAAkB,CAAEA,CAACb,KAAK,CAAEE,MAAM,GAAK,CACrCF,KAAK,CAACL,aAAa,CAAGK,KAAK,CAACL,aAAa,CAACmB,MAAM,CAC9CC,YAAY,EAAIA,YAAY,CAACL,EAAE,GAAKR,MAAM,CAACC,OAC7C,CAAC,CACH,CAAC,CACDa,kB